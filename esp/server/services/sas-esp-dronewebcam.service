[Unit]
Description=IOT demo
Requires=network-online.target
After=network-online.target

[Service]
User=ec2-user
ExecStart=/bin/sh -c '/app/iotdemo/esp/server/objectdetection-server.sh -a 30005 -p 30006 -m /app/iotdemo/astore/DoD_warehouse/yolov2.astore -s /app/iotdemo/astore/DoD_warehouse/schema.txt > /app/logs/esp-dronewebcam.log 2>&1'

[Install]
WantedBy=multi-user.target
